{
  "name": "new-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:https": "next dev --experimental-https --experimental-https-key /home/mshittu/certs/tailscale_key.pem --experimental-https-cert /home/mshittu/certs/tailscale_cert.pem",
    "build": "next build",
    "start": "next start",
    "prettier": "prettier \"**/*.+(json|ts|tsx)\"",
    "format:fix": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check",
    "types:check": "tsc --noEmit --pretty",
    "lint": "eslint --ext .ts,.tsx ./src ",
    "lint:fix": "eslint --ext .ts,.tsx ./src --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky",
    "prisma:format": "dotenv -e .env.local -- npx prisma format",
    "prisma:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
    "prisma:push": "dotenv -e .env.local -- npx prisma db push",
    "prisma:studio": "dotenv -e .env.local -- npx prisma studio",
    "prisma:generate-schema": "dotenv -e .env.local -- ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/generate-schema.ts",
    "prisma:generate-schema:prod": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/generate-schema.ts",
    "prisma:generate-schema:prod-setup": "dotenv -e .env.prod -- ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/generate-schema.ts",
    "prisma:migrate:dev": "dotenv -e .env.local -- npx prisma migrate dev --schema=prisma/schema.prisma",
    "prisma:generate": "npx prisma generate --schema=prisma/schema.prisma",
    "prisma:seed:dev": "dotenv -e .env.local -- npx ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "prisma:seed:prod": "npx ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "db:reset:dev": "dotenv -e .env.local -- npx prisma migrate reset --force",
    "db:reset:prod": "npx prisma migrate reset --force",
    "db:reset:prod-setup": "dotenv -e .env.prod -- npx prisma migrate reset --force",
    "prisma:generate-client:dev": "dotenv -e .env.local -- yarn prisma generate",
    "prisma:generate-client:prod": "dotenv -e .env.prod -- yarn prisma generate",
    "cron:auto-end": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' src/lib/cron/autoEndSessions.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.1",
    "@prisma/client": "^5.22.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.1",
    "@tanstack/react-query": "^5.60.6",
    "@tanstack/react-query-devtools": "^5.60.6",
    "axios": "^1.7.7",
    "csv-stringify": "^6.5.2",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.14.4",
    "next": "15.0.3",
    "react": "19.0.0-rc-66855b96-20241106",
    "react-dom": "19.0.0-rc-66855b96-20241106",
    "react-toastify": "^10.0.6",
    "tailwind-merge": "^2.5.5",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@faker-js/faker": "^9.2.0",
    "@storybook/addon-essentials": "^8.4.2",
    "@storybook/addon-interactions": "^8.4.2",
    "@storybook/addon-onboarding": "^8.4.2",
    "@storybook/blocks": "^8.4.2",
    "@storybook/nextjs": "^8.4.2",
    "@storybook/react": "^8.4.2",
    "@storybook/test": "^8.4.2",
    "@tanstack/eslint-plugin-query": "^5.60.1",
    "@types/date-fns": "^2.6.3",
    "@types/node": "^24.0.10",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "daisyui": "^4.12.14",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-storybook": "^0.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "storybook": "^8.4.2",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
